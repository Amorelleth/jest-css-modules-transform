{
  "name": "jest-css-modules-transform",
  "description": "Jest's preprocessor for css, sass, less, stylus modules generated with Webpack",
  "version": "1.1.0",
  "license": "MIT",
  "author": "Mikhail Bodrov",
  "engines": {
    "node": ">=6.4.0"
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/jest-css-modules-transform.git"
  },
  "keywords": [
    "jest",
    "webpack",
    "css-import",
    "transform",
    "preprocessor",
    "class",
    "classname",
    "css",
    "stylus",
    "sass",
    "scss",
    "less",
    "module",
    "css-module"
  ],
  "scripts": {
    "coverage": "jest --coverage --config jest.json",
    "lint": "eslint src",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint && npm test",
    "prepare": "npm run lint && npm test",
    "test": "jest --no-cache --config jest.json && jest --no-cache --config jest-with-css-config.json"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-plugin-jest": "21.15.0",
    "husky": "0.14.3",
    "jest-cli": "22.4.3",
    "less": "3.0.1",
    "node-sass": "4.8.3",
    "stylus": "^0.54.5"
  }
}
